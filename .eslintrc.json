{
  "env": {
    "node": true
  },
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "indent": [2, 2, {
      "SwitchCase": 1,
      "MemberExpression": 1
    }],
    "quotes": [2, "single"],
    "dot-notation": [2, {"allowKeywords": true}]
  }
}
